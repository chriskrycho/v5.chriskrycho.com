   "devDependencies": {
      "@11ty/eleventy": "^2.0.1",
      "@rollup/plugin-node-resolve": "^15.0.2",
      "@rollup/plugin-strip": "^3.0.2",
      "@rollup/plugin-terser": "^0.4.1",
      "@rollup/plugin-typescript": "^11.1.0",
      "@types/gulp-dart-sass": "^1.0.1",
      "@types/luxon": "^3",
      "@types/markdown-it": "^12.2.3",
      "@types/markdown-it-container": "^2.0.4",
      "@types/node": "^16.11.7",
      "@types/sass": "^1.43.1",
      "@types/serve-static": "^1.15.0",
      "@types/uslug": "^1.0.1",
      "del": "^6.0.0",
      "gulp": "^4.0.2",
      "gulp-dart-sass": "^1.0.2",
      "highlight.js": "^11.7.0",
      "highlightjs-glimmer": "^2.0.1",
      "luxon": "^3",
      "markdown-it": "^13.0.1",
      "markdown-it-abbr": "^1.0.4",
      "markdown-it-anchor": "^8.6.5",
      "markdown-it-deflist": "^2.1.0",
      "markdown-it-footnote": "^3.0.3",
      "markdown-it-implicit-figures": "^0.10.0",
      "markdown-it-sup": "^1.0.0",
      "prettier": "^2.8.7",
      "rollup": "^3.20.7",
      "sass": "^1.62.0",
      "striptags": "^3.2.0",
      "true-myth": "^4.1.1",
      "ts-node": "^10.9.1",
      "tslib": "^2.5.0",
      "typescript": "^5.0.4",
      "typeset": "^0.3.3",
      "uslug": "^1.0.4"
   },
   "prettier": {
      "singleQuote": true,
      "printWidth": 90,
      "trailingComma": "all"
   },
   "scripts": {
      "build:styles": "gulp all",
      "build:site": "eleventy --quiet",
      "build:scripts": "rollup -c rollup.config.mjs",
      "build": "npm run build:scripts && npm run build:styles && npm run build:site -- --quiet",
      "watch:styles": "gulp watch",
      "watch:site": "eleventy --serve",
      "watch:scripts": "rollup --config rollup.config.js --watch",
      "watch": "npm run watch:site && npm run watch:styles",
      "serve": "env DEV=true eleventy --quiet --serve --incremental",
      "clean": "rm -rf public"
   },
   "dependencies": {
      "markdown-it-mark": "^3.0.1"
   }
}
